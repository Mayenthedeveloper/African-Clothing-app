  <div className='product-name'>\n          <Link to={'/product/' + product.id}>{product.name}</Link>\n          </div>\n          <div className='product-brand'>{product.brand}</div>\n          <div className='product-size'>{product.size}</div>\n          <div className='product-price'>${product.price}</div>\n        </div>\n      </li>)\n      \n        }\n      \n    </ul>\n    )\n}\n\nexport default HomeScreen;","/Users/mayenakpan/projects/african-clothing/src/Screens/ProductScreen.js",[],"/Users/mayenakpan/projects/african-clothing/src/Screens/CartScreen.js",["75","76","77","78","79"],"import React, {useContext, useState} from 'react';\nimport { Link } from 'react-router-dom';\nimport AppContext from '../AppContext'\nimport TokenService from '../services/token-service'\nimport OrderConfirmation from './OrderConfirmation'\n\nfunction CartScreen (props){\n\n    const [userLoginStatus, setStatus] = useState(\"Place Order\")\n    const userLoggedin = props.location.state\n    var userHasLoggedIn = \"\"\n\n    const context = useContext(AppContext)\n    var itemList = []\n    context.cart.forEach(item=> {\n        if(!(item.id in itemList))\n        {\n            var itemid = item.id\n            itemList[itemid] =  item\n        }\n        else{\n            var tempItem = itemList[item.id]\n            tempItem.quantity = parseInt(tempItem.quantity) + parseInt(item.quantity); //typecasting \n            itemList[item.id] = tempItem\n        }\n    })\n    \n    if(userLoggedin != \"loggedin\")\n    {\n        userHasLoggedIn = false\n    }\n    else\n    {\n        userHasLoggedIn = true\n    }\n\n\n    \n    // const productId = props.match.params.id;\n    var finalAmount = 0\n    context.cart.forEach(item=> {\n        finalAmount += item.price\n    })\n    const [total, setTotal] = useState(finalAmount)\n    \n    const setTot = () => {\n        setTotal(finalAmount)\n    }\n\n    const removeItem = (itemId) => {\n            var elementName = \"pro\" + itemId\n            document.getElementById(elementName).style.display = \"none\"\n            finalAmount = finalAmount - itemList[itemId].price\n            setTot()\n    }\n  \n\n    return(\n        <div className='cart'>\n            <div className='cart-list'>\n                        <h3>\n                            Shopping Cart\n                        </h3>\n\n                    <table id=\"cartTable\">\n                       \n                    {\n                        itemList.map(item=>\n                            <tr id = {\"pro\"+item.id}>\n                               <td >\n                                    <img className=\"cart-image\" src={item.image} alt=\"product\" />\n                                </td> \n                                <td>\n                                    <Link to={\"/product/\" + item.id}>\n                                    {item.name}\n                                    </Link>\n                                </td>\n                                <td>\n                                    Quantity:\n                                <select value = {item.quantity}>\n                                    <option>1</option>\n                                    <option>2</option>\n                                    <option>3</option>\n                                    <option>4</option>\n                                    <option>5</option>\n                                </select>\n                                </td>\n                                <td>\n                                <button type=\"button\" className=\"button\" onClick={(e)=> removeItem(item.id)} >\n                                    Delete\n                                </button>\n                            </td>\n                            <td className=\"cart-price\">\n                                ${item.price}\n                            </td>\n                            </tr>\n                        )\n                    }\n                    </table>\n\n                <div>\n                    <h3>\n                        Subtotal : $ {total}\n                    </h3>\n                    {\n                        userHasLoggedIn ? \n                        <Link to='/orderConfirmation' product = {context.cart}>\n                            <button> Finish Placing Order </button>\n                        </Link>\n                        :\n                        <Link to='/signin' product = {context.cart}>\n                            <button> Place Order </button>\n                      </Link>\n                    }\n                </div>\n\n            </div>\n        </div>\n    )\n}\n\nexport default CartScreen;","/Users/mayenakpan/projects/african-clothing/src/Screens/SigninScreen.js",["80","81","82"],"/Users/mayenakpan/projects/african-clothing/src/Screens/RegisterScreen.js",["83","84"],"import React, {useState} from 'react';\nimport { Link } from 'react-router-dom';\nimport config from '../config'\nimport AuthApiService from '../services/auth-api-service'\n\nfunction RegisterScreen(props){\n\n    const user = {}\n    // const [input, setInput] = useState({\n    //     name: '',\n    //     email: '',\n    //     password: '',\n    //     rePassword: ''\n    // })\n    const [name, setName] = useState('')\n    const [email, setEmail] = useState('')\n    const [password, setPassword] = useState('')\n    const [error, setError] = useState('')\n\n   const handleSubmit = (e) =>{\n        e.preventDefault();\n       user.name = name\n       user.email = email\n       user.password = password\n       AuthApiService.postUser({\n           name,\n           email,\n           password\n       })\n       .then(()=>{\n           setName('')\n           setEmail('')\n           setPassword('')\n           props.history.push('/signin')\n       })\n\n       .catch(res => {\n        setError(res.error)\n      })\n        // fetch(`${config.API_ENDPOINT}/users`, {\n        //     method: 'POST',\n        //     headers: {\n        //     'content-type': 'application/json'\n        //     },\n        //     body: JSON.stringify(user),\n        //     })\n        //     .then(res => {\n        //         console.log(\"user adedd\")\n        //     })\n        //     .catch(error => {\n        //     console.error({ error })\n        // })\n}\n\n\n   const handleInputChange =(e)=>{ \n    e.preventDefault();\n    \n}\n\n   \n\n    return(\n        <div className='form' onSubmit={handleSubmit}>\n            <form>\n                <ul>\n                    <li>\n                        <h2>Create Account</h2>\n                    </li>\n                    <li>\n                        <label htmlFor='name'>\n                            Name\n                        </label>\n                        <input type='name' id='name'  name='name' onChange={e => setName(e.target.value)}  required></input>\n                    </li>\n                    <li>\n                        <label htmlFor='email'>\n                            Email\n                        </label>\n                        <input type='email' id='email' name='email' onChange={e => setEmail(e.target.value)}></input>\n                    </li>\n                    <li>\n                        <label htmlFor='password'>\n                            Password\n                        </label>\n                        <input type='password' id='password' name='password' onChange={e => setPassword(e.target.value)} ></input>\n                    </li>\n                    <li>\n                        <label htmlFor='rePassword'>\n                            Re-Enter-Password\n                        </label>\n                        <input type='password' id='rePassword' name='rePassword' onChange={handleInputChange} ></input>\n                    </li>\n                    <li>\n                        <button type=\"submit\" className=\"button primary\" onClick = {handleSubmit}>\n                            Register\n                        </button>\n                    </li>\n                    <li>\n                        Already have an account?\n                        \n                    </li>\n                    <li>\n            \n                    <Link to='/signin'>Sign In</Link>\n                    </li>\n                </ul>\n            </form>\n        </div>\n    )\n}\n\nexport default RegisterScreen;","/Users/mayenakpan/projects/african-clothing/src/AppContext.js",[],"/Users/mayenakpan/projects/african-clothing/src/config.js",["85"],"export default {\n     API_KEY: process.env.REACT_APP_API_KEY,\n     API_ENDPOINT: 'http://localhost:8000/api',\n     TOKEN_KEY: 'eafa24be'\n     }","/Users/mayenakpan/projects/african-clothing/src/services/auth-api-service.js",[],"/Users/mayenakpan/projects/african-clothing/src/services/idle-service.js",[],"/Users/mayenakpan/projects/african-clothing/src/services/token-service.js",[],"/Users/mayenakpan/projects/african-clothing/src/Screens/OrderConfirmation.js",["86","87","88","89","90"],"import React, {useContext, useState} from 'react';\nimport { Link } from 'react-router-dom';\nimport data from '../data';\nimport AppContext from '../AppContext'\n\nfunction OrderConfirmation(props){\n\n    return (\n        <div>\n            Success!! Order was placed\n        </div>\n    )\n}\n\nexport default OrderConfirmation","/Users/mayenakpan/projects/african-clothing/src/Header/Header.js",[],{"ruleId":"91","severity":1,"message":"92","line":7,"column":8,"nodeType":"93","messageId":"94","endLine":7,"endColumn":20},{"ruleId":"95","replacedBy":"96"},{"ruleId":"97","replacedBy":"98"},{"ruleId":"91","severity":1,"message":"99","line":12,"column":8,"nodeType":"93","messageId":"94","endLine":12,"endColumn":14},{"ruleId":"91","severity":1,"message":"100","line":21,"column":20,"nodeType":"93","messageId":"94","endLine":21,"endColumn":31},{"ruleId":"91","severity":1,"message":"101","line":8,"column":9,"nod