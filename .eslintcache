,"source":"65","usedDeprecatedRules":"33"},"/Users/mayenakpan/projects/african-clothing/src/index.js",["66"],"\n\nimport React from 'react';\nimport ReactDOM from 'react-dom';\nimport { BrowserRouter } from 'react-router-dom';\nimport App from './App';\nimport SigninScreen from './Screens/SigninScreen'\nimport './index.css';\n\nReactDOM.render(\n<BrowserRouter>\n  <App />\n</BrowserRouter>,\ndocument.getElementById('root'));\n\n",["67","68"],"/Users/mayenakpan/projects/african-clothing/src/App.js",["69","70"],"import React,{useState, useEffect} from 'react';\nimport  {  Route, Link } from 'react-router-dom';\n// import data from './data';\nimport HomeScreen from './Screens/HomeScreen';\nimport ProductScreen from './Screens/ProductScreen';\nimport CartScreen from './Screens/CartScreen';\nimport './App.css';\nimport SigninScreen from './Screens/SigninScreen';\nimport RegisterScreen from './Screens/RegisterScreen';\nimport AppContext from './AppContext';\nimport OrderConfirmation from './Screens/OrderConfirmation'\nimport config from './config'\n\n\n\n\nfunction App() {\n  const [cart, setCart] = useState([])\n  const [products, setProducts] = useState([])\n  useEffect(() => {\n    // fetch(`${config.API_ENDPOINT}/products`)\n    // .then(res => res.json())\n    // .then(products => setProducts(products))\n    // setProducts(data)\n  }, [])\n\n  const openMenu = ()=> {\n    document.querySelector('.sidebar').classList.add('open');\n\n  };\n\n  const closeMenu = ()=> {\n    document.querySelector('.sidebar').classList.remove('open');\n  };\n\n  const addToCart = (item)=>{\n    setCart([...cart, item])\n  }\n\n  const value = {\n    cart,\n    addToCart,\n    products\n  }\n\n  return (\n    <AppContext.Provider value ={value}>\n    <div className='grid-container'>\n      <header className='header'>\n        <div className='brand'>\n          <button onClick={openMenu}>\n            &#9776;\n          </button>\n        </div>\n        <div className='header-links'>\n          <Link to='/'>African Clothing</Link>\n        </div>\n        <div className='header-links'>\n           <Link to='/cart'> Cart-{cart.length}</Link>\n          <Link to='/signin'> Sign In </Link>\n          <Link/>\n        </div>\n      </header>\n      <aside className='sidebar'>\n        <h3>Shopping Categories</h3>\n        <button className='sidebar-close-button' onClick={closeMenu}>X</button>\n        <ul className='categories'>\n          <li key=\"category1\"><Link to='/category/Skirt and Blouse'>Skirt and Blouse</Link></li>\n          <li key=\"category2\"><Link to='/category/Gown'>Gown</Link></li>\n          <li key=\"category3\"><Link to='/category/Blouse'>Blouse</Link></li>\n          <li key=\"category4\"><Link to='/category/Skirt'>Skirt</Link></li>\n        </ul>\n      </aside>\n     \n      <main className ='main'>\n        <div className='content'>\n          <Route path='/product/:id' component={ProductScreen}/>\n          <Route path='/signin' component={SigninScreen}/>\n          <Route path='/register' component={RegisterScreen}/>\n          <Route path='/cart/:id?' component={CartScreen}/>\n          <Route path=\"/category/:id\" component={HomeScreen} />\n          <Route path='/' exact component={HomeScreen}/>\n          <Route path = \"/orderConfirmation\" component = {OrderConfirmation}/>\n          \n        </div>\n        \n        </main>\n\n    <footer className='footer'>All right reserved</footer>\n    </div>\n    </AppContext.Provider>\n  );\n}\n\nexport default App;","/Users/mayenakpan/projects/african-clothing/src/data.js",[],"/Users/mayenakpan/projects/african-clothing/src/Screens/HomeScreen.js",["71"],"import React,{useContext} from 'react';\nimport { Link } from 'react-router-dom';\nimport data from '../data';\nimport AppContext from '../AppContext'\nimport TokenService from '../services/token-service'\n\nfunction HomeScreen(props){\n  const context = useContext(AppContext)\n\n  const {id} = props.match.params\n  // let products = context.products\n  let products = data.products\n  if(id){\n    //  products = context.products.filter(product=> product.category === id)\n    products = data.products.filter(product=> product.category === id)\n  }\n   \n  try {\n      //console.log(\"Auth token \" + TokenService.getAuthToken())\n      console.log(\"JWT token \")\n      console.log(TokenService.readJwtToken())\n  }\n  catch(error)\n  {\n    console.log(\"No valid token yet\")\n  }\n\n\n    return(\n        <ul className='products'>\n        {\n         products.map(product => \n      <li key = {product.id}>\n        <div className='product'>\n        <Link to={'/product/' + product.id}>\n          <img className ='product-image' src={product.image} alt='product'/>\n        </Link>\n          \n          <div className='product-name'>\n          <Link to={'/product/' + product.id}>{product.name}</Link>\n          </div>\n          <div className='product-brand'>{product.brand}</div>\n          <div className='product-size'>{product.size}</div>\n          <div className='product-price'>${product.price}</div>\n        </div>\n      </li>)\n      \n        }\n      \n    </ul>\n    )\n}\n\nexport default HomeScreen;","/Users/mayenakpan/projects/african-clothing/src/Screens/ProductScreen.js",[],"/Users/mayenakpan/projects/african-clothing/src/Screens/CartScreen.js",["72","73","74","75","76"],"/Users/mayenakpan/projects/african-clothing/src/Screens/SigninScreen.js",["77","78","79","80"],"import React, { useState} from 'react';\nimport { Link } from 'react-router-dom';\nimport AuthApiService from '../services/auth-api-service'\nimport TokenService from '../services/token-service'\nimport IdleService from '../services/idle-service'\n\n\nfunction SigninScreen(props){\n    const [email, setEmail] = useState('')\n    const [password, setPassword] =useState('')\n    const [error, setError] = useState('')\n\n    \n   const submitHandler = (e)=>{\n        e.preventDefault();\n        setError(null) \n\